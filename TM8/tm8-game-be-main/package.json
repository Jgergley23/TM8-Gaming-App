{
  "name": "tm8",
  "version": "0.0.1",
  "description": "TM8 - Game Matchmaking",
  "author": "Valere Margins",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "npx compodoc -p tsconfig.json -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-ssm": "^3.556.0",
    "@faker-js/faker": "^8.4.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/websockets": "^9.0.0",
    "@sendgrid/mail": "^8.1.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/bcrypt": "^5.0.2",
    "argon2": "^0.31.2",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-basic-auth": "^1.2.1",
    "express-correlation-id": "^2.0.1",
    "firebase-admin": "^12.0.0",
    "google-auth-library": "^9.6.3",
    "helmet": "^7.1.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.1",
    "nest-typed-config": "^2.9.1",
    "nest-winston": "^1.9.4",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.14",
    "request-ip": "^3.3.0",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.5",
    "stream-chat": "^8.23.0",
    "twilio": "^4.23.0",
    "verify-apple-id-token": "^3.1.2",
    "winston-daily-rotate-file": "^4.7.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "@types/jest": "29.5.0",
    "@automock/jest": "^2.1.0",
    "@automock/adapters.nestjs": "^2.1.0",
    "@nestjs/testing": "^9.4.3",
    "supertest": "^6.1.3",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.11",
    "@types/supertest": "^2.0.11",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@compodoc/compodoc": "^1.1.22",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/node": "18.15.11",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
